<h1>ログイン</h1>

<%= form_with url: login_path, local: true do |f| %>
  <div>
    <%= label_tag :student_no, "学籍番号" %><br>
    <%= text_field_tag :student_no, params[:student_no] %>
  </div>
  <div>
    <%= label_tag :password, "パスワード" %><br>
    <%= password_field_tag :password %>
  </div>
  <div>
    <%= submit_tag "ログイン" %>
  </div>
<% end %>

